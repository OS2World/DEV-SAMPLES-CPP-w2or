DLGTEMPLATE DIALOG_SETUP 
BEGIN
  DIALOG "Cron setup", DIALOG_SETUP, 67,53,139,82, WS_VISIBLE|FS_DLGBORDER, FCF_TITLEBAR|FCF_BORDER|FCF_SYSMENU|FCF_SCREENALIGN|FCF_DLGBORDER
  PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
  BEGIN
    CONTROL "Setup", 0xffff, 4,41,131,37,"BLShade",WS_TABSTOP|WS_DISABLED|WS_GROUP|0x0FFF
    LTEXT "Check crontab every", 0xffff, 8,59,74,8,WS_GROUP|DT_WORDBREAK
    EDITTEXT "", ID_MINUTEINTERVAL, 83,57,21,12,WS_TABSTOP|ES_LEFT|ES_AUTOSCROLL|ES_MARGIN
    LTEXT "minute", 0xffff, 106,59,24,8,WS_GROUP|DT_WORDBREAK
    CONTROL "Active", ID_ACTIVE, 9,44,49,10,"BLcb_def",WS_TABSTOP|WS_DISABLED|WS_GROUP|0x0FFF
    CONTROL "&Ok", DID_OK, 7,9,33,21,"BLbtn",WS_TABSTOP|WS_DISABLED|WS_GROUP|0x0FFF
    CONTROL "&Cancel", DID_CANCEL, 49,9,33,21,"BLbtn",WS_TABSTOP
    CONTROL "", 0xffff, 0,33,139,2,"BLShade",WS_TABSTOP|WS_DISABLED|WS_GROUP|0x0FFF
    CONTROL "&Save", ID_SAVEBUTTON, 92,10,32,20,"BLbtn",WS_TABSTOP|WS_DISABLED|WS_GROUP|0x0FFF
  END
END


DLGTEMPLATE DIALOG_ABOUT 
BEGIN
